{
  "name": "zod-defaults",
  "version": "0.2.3",
  "description": "Generate an 'empty' default object from a zod schema, mainly meant for initial state of a form.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "biome check src",
    "test": "vitest --run",
    "build": "npm run build:js && npm run build:types",
    "build:js": "bun build ./src/index.ts --outfile=dist/index.js --format=esm --target=browser",
    "build:types": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "peerDependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/node": "^24.7.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}
